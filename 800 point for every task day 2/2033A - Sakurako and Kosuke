t = int(input())
for _ in range(t):
    n = int(input())
    x = 0
    i = 1
    while -n <= x <= n:
        d = 2 * i - 1
        if i % 2 == 1:  
            x -= d
            if x < -n:
                print("Sakurako")
                break
        else:  
            x += d
            if x > n:
                print("Kosuke")
                break
        i += 1
