for _ in range(int(input())):
    n = int(input())
    a = list(map(int, input().split()))
    for i in range(2, n):
        if a[i] != a[0] and a[i] != a[1]:
            print(i + 1)
            break
        if a[0] != a[1] and a[0] != a[2]:
            print(1)
            break
        if a[1] != a[0] and a[1] != a[2]:
            print(2)
            break
