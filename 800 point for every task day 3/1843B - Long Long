t = int(input())
for _ in range(t):
    n = int(input())
    a = list(map(int, input().split()))
    
    total = sum(abs(x) for x in a)
    op = 0
    i = 0
    while i < n:
        if a[i] < 0:
            op += 1
            while i < n and a[i] <= 0:
                i += 1
        else:
            i += 1
    print(total, op)
