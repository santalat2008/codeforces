t = int(input())
for _ in range(t):
    n = int(input())
    s = input()
    res = []
    i = 0
    while i < n:
        if s[i] == 'U':
            res.append('D')
            i += 1
        elif s[i] == 'D':
            res.append('U')
            i += 1
        elif s[i] == 'L':
            res.append('L')
            res.append('R')
            i += 2
        elif s[i] == 'R':
            i += 1
    print(''.join(res))
