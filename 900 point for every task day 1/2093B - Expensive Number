T = int(input())
for _ in range(T):
    N = input()
    nz = False
    cnt = 0
    for i in range(len(N)-1, -1, -1):
        if N[i] == '0':
            if not nz:
                cnt += 1
        else:
            if not nz:
                nz = True
            else:
                cnt += 1
    print(cnt)
