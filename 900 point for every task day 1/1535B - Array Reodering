t = int(input())
for _ in range(t):
    n = int(input())
    a = list(map(int, input().split()))
    e = [x for x in a if x % 2 == 0]
    o = [x for x in a if x % 2 == 1]
    cnt = 0
    a = e + o
    for i in range(n):
        for j in range(i + 1, n):
            if math.gcd(a[i], 2 * a[j]) > 1:
                cnt += 1
    print(cnt)
