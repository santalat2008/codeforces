n = int(input())
a = list(map(int, input().split()))
from collections import defaultdict
d = defaultdict(list)
for i in range(n):
    s = 0
    for j in range(i, n):
        s += a[j]
        d[s].append((i, j))
res = []
for v in d.values():
    v.sort(key=lambda x: x[1])
    tmp = []
    end = -1
    for l, r in v:
        if l > end:
            tmp.append((l, r))
            end = r
    if len(tmp) > len(res):
        res = tmp
print(len(res))
for l, r in res:
    print(l + 1, r + 1)
